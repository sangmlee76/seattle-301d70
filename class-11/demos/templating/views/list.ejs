<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>List</title>
</head>
<body>
  <h1>List</h1>
  <ul>

    <% for(let i = 0; i < theListKeyName.length; i++){ %>

      <li><%= theListKeyName[i].product %> x <%=theListKeyName[i].quantity %></li>

    <% } %>

  </ul>

  <ul>
    <% for(let i = 0; i < names.length; i++){ %>

        <li> <%= names[i] %> </li>

    <% } %>
  </ul>

  <% console.log('from ejs', names) %>

  <ul>
    <% names.forEach(name => { %>
      <li>Name : <%= name %> </li>
    <% }) %> 
  </ul>
</body>
</html>